HELLO WORLD!!


    <h2>{{users.first_name}}</h2>
    <a class="waves-effect waves-light btn modal-trigger" href="#modal1">New Blog Post</a>
  {{#each users.blogs as |blogs|}}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <h2>{{blogs.title}} </h2>
    <h2>{{blogs.content}} </h2>

  </div>
</div>
{{/each}}

  <!-- Modal Trigger -->
  

  <!-- Modal Structure -->
  <div id="modal1" class="modal">
    <div class="modal-content">
      <div class="col-md-6">
    <h2>Post a new blog!</h2>

    <form class="form">
      <div class="form-group">
        <label for="blog-title">Title</label>
        <input class="form-input" type="text" id="blog-title" />
      </div>
      <div class="form-group">
        <label for="blog-content">Post</label>
        <textarea class="form-input" rows="20" type="text" id="blog-content"></textarea>
      </div>
      <div class="form-group">
        <button class="btn btn-primary" id="newPost" type="submit">Post Blog</button> <a href="#!" class="modal-close waves-effect waves-green btn-flat">Cancel</a>
      </div>
    </form>
 
</div>
    </div>
  </div>